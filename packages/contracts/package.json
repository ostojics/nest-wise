{
  "name": "@nest-wise/contracts",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "lint": "eslint src/**/*.ts",
    "clean": "rm -rf dist"
  },
  "devDependencies": {
    "tsup": "catalog:",
    "typescript": "catalog:",
    "@nest-wise/linting": "workspace:*",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "@nest-wise/tsconfig": "workspace:*",
    "zod": "catalog:"
  }
}
