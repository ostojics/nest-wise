{
  "name": "@nest-wise/locales",
  "version": "0.0.1",
  "private": true,
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "lint": "eslint src/**/*.ts"
  },
  "devDependencies": {
    "@nest-wise/linting": "workspace:*",
    "@nest-wise/tsconfig": "workspace:*",
    "eslint": "catalog:",
    "tsup": "catalog:",
    "typescript": "catalog:"
  }
}
